{
  "name": "Copy It",
  "description": "Copy url and title by shortcut key",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "background.js"
      ],
      "all_frames": true
    }
  ]
}
